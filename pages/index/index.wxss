.container{
  padding-bottom: 158rpx;
}
.home-swiper {
  width: 750rpx;
  height: 400rpx;
}
.home-swiper image {
  width: 750rpx;
  height: 400rpx;
}
.swiper-box {
  position: relative;
} 
.swiper-location {
  position: absolute;
  left:30rpx;
  top:26rpx;
  color:#fff;
  z-index: 1;
  font-size: 24rpx;
  line-height: 34rpx;
  padding-left: 24rpx;
  background: url(data:image/png;base64,) no-repeat left center;
  background-size: 18rpx 22rpx;
}
.search-bar {
  width: 750rpx;
  height: 80rpx;
  position: absolute;
  left: 0;
  bottom:-40rpx;
  display: flex;
  justify-content: center;
}
.search-input {
  width:690rpx;
  height: 80rpx;
  font-size: 28rpx;
  color:#fff;
  border-radius:51rpx;
  padding: 0 94rpx;
  background: rgba(5,5,18,0.5) url(data:image/png;base64,) no-repeat 32rpx center;
  background-size:37rpx 37rpx;
}
.context-box {
  padding: 80rpx 30rpx 0 30rpx;
}
.booking-swiper {
  height: 330rpx;
}
.booking-frame {
  height: 330rpx;
  background-color: #111841;
  position: relative;
}
.bg-booking {
  position: absolute;
  top:0;
  left:0;
  width:100%;
  height: 200rpx;
  border-radius: 4rpx;
}
.booking-bar-farme {
  padding: 44rpx 40rpx 36rpx 30rpx;
  background:linear-gradient(225deg,rgba(198,0,135,1) 0%,rgba(0,108,209,1) 100%);
  opacity:0.8;
  display: flex;
  justify-content: space-between;
  height: 200rpx;
  border-radius: 4rpx;
}
.booking-text {
  display: flex;
  padding-bottom: 20rpx;
  align-items: flex-end;
}
.booking-frame-activity{
  width:100%;
}
.booking-text-activity{
  display: flex;
  padding-bottom: 20rpx;
  align-items: flex-end;
  justify-content: space-between;
  width:100%;
}
.booking-activity-name {
  font-size: 40rpx;
  color:rgba(255, 255, 255, 1);
  font-weight: 500;
}
.booking-name {
  font-size: 40rpx;
  font-weight: 500;
  color:#fff;
}
.booking-info {
  padding-left: 18rpx;
  font-size: 24rpx;
  color:rgba(255, 255, 255, .8);
  display: flex;
  align-items: center;
  padding-bottom: 6rpx;
}
.vertical-line {
  width: 2rpx;
  height: 26rpx;
  background-color: #fff;
  margin: 0 10rpx;
}
.booking-btn {
  padding:0 12rpx 0 42rpx;
  height: 40rpx;
  line-height: 38rpx;
  border-radius:4rpx;
  width:130rpx;
  border:1rpx solid rgba(255,255,255,.5);
  font-size: 24rpx;
  color:#fff;
}
.booking-btn.success{
  background: url(data:image/png;base64,) no-repeat 12rpx center;
  background-size: 24rpx 24rpx;
}

.booking-btn.wait{
  background: url(data:image/png;base64,) no-repeat 12rpx center;
  background-size: 24rpx 24rpx;
}

.booking-btn.warn{
  background: url(data:image/png;base64,) no-repeat 12rpx center;
  background-size: 24rpx 24rpx;
}

.booking-frame-r {
  width:120rpx;
  height: 120rpx;
  border-radius: 4rpx;
  border:1rpx solid rgba(255,255,255,.5);
  padding: 14rpx 0;
}
.booking-frame-r view:last-child {
  color:#fff;
  font-size: 24rpx;
  text-align: center;
}
.booking-num {
  font-size: 40rpx;
  font-weight: 500;
  color:#fff;
  text-align: center;
}
.booking-text-frame {
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 130rpx;
}
.booking-time {
  font-size: 24rpx;
  color:rgba(255, 255, 255, .8);
  line-height: 34rpx;
  padding-bottom: 18rpx;
}
.booking-delay {
  font-size: 24rpx;
  color:rgba(255, 255, 255, .8);
  line-height: 34rpx;
  display: flex;
}
.booking-delay-tips {
  font-size: 20rpx;
  color:rgba(255, 255, 255, .6);
}
.hot-bar-title {
  padding-top:60rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.hot-bar-title > view{
  display: flex;
  align-items: center;
}
.icon-hot {
  width:34rpx;
  height: 34rpx;
}
.hot-bar-text{
  color:#fff;
  font-size:30rpx;
  font-weight: 500;
  padding-left: 6rpx;
}
.hot-bar-item {
  display: flex;
  padding-top:42rpx;
  height: 192rpx;
}
.hot-bar-image {
  width:196rpx;
  height: 150rpx;
  border-radius: 8rpx;
}
.hot-bar-context {
  padding-left: 16rpx;
  flex: 1;
}
.hot-bar-context>view{
  display: flex;
  color:#fff;
  justify-content: space-between;
}
.hot-bar-context>view:nth-child(1){
  padding-bottom: 30rpx;
}
.hot-bar-text {
  font-size: 32rpx;
  font-weight: 500;
  color:rgba(255, 255, 255, .8);
}
.hot-bar-time {
  font-size: 24rpx;
  padding-left: 8rpx;
  color:rgba(255, 255, 255, .6);
  display: flex;
  align-items: center;
}
.hot-bar-adre {
  font-size: 24rpx;
  padding-left: 8rpx;
  color:rgba(255, 255, 255, .6);
  padding-top: 10rpx;
  display: flex;
  align-items: center;
  width:382rpx;
}
.adre-text {
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  width:300rpx;
}
.hot-bar-distance {
  font-size: 24rpx;
  padding-left: 8rpx;
  color:rgba(255, 255, 255, .6);
  padding-top: 10rpx;
}
.time-text {
  padding-right: 10rpx;
}
.time-text:last-child{
  padding-right: 0;
}
/* 设置点点的层级 ,可以调整小点的位置*/
.wx-swiper-dots{position:relative;top:326rpx !important;}
/* 修改vant下弹出框样式样式start */
.van-picker__toolbar{
  background:rgba(21,21,62,1);
  height: 100rpx !important;
  box-shadow:0px -1rpx 0 0 rgba(255,255,255,0.1);
  border-radius:40rpx 40rpx 0 0 !important;
}
.van-picker__cancel{
  font-size: 30rpx !important;
  color:rgba(255,255,255,.5) !important;
  line-height: 100rpx !important;
}
.van-picker__confirm{
  font-size: 30rpx !important;
  color:rgba(255,255,255,1) !important;
  line-height: 100rpx !important;
}
.van-picker__title {
  color:rgba(255,255,255,.8);
  font-size: 30rpx !important;
  line-height: 100rpx;
}
.van-picker{
  background-color:#12122F !important;
  border-radius:40rpx 40rpx 0 0 !important;
}
.van-picker-column__item{
  font-size: 28rpx;
}
.van-picker-column__item--selected{
  font-size: 34rpx;
  font-weight: 500 !important;
  color:#fff !important;
}
.van-picker__toolbar::after {
  border-bottom: 1rpx  solid rgba(255,255,255,.1) !important;
  border-top:none !important;
}
.van-picker__cancel--hover,.van-picker__confirm--hover{
  background-color: rgba(21,21,62,1) !important;
}
.van-picker__frame::after {
  content: '';
  width:750rpx !important;
  height: 1rpx !important;
  background: linear-gradient(to right, black , rgba(255, 255, 255, .5), black) !important;
  position: absolute;
  top:0 !important;
  left:0 !important;
  transform: scale(1) !important;
  border:none !important;
}
.van-picker__frame::before {
  content: '';
  width:750rpx;
  height: 1rpx;
  background: linear-gradient(to right, black , rgba(255, 255, 255, .5), black);
  position: absolute;
  top:44px;
  left:0;
}
.van-popup{
  background-color: transparent !important;
}
/* 修改vant样式end */