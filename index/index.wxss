.home-swiper {
  width: 750rpx;
  height: 400rpx;
}
.home-swiper image {
  width: 750rpx;
  height: 400rpx;
}
.swiper-box {
  position: relative;
} 
.swiper-location {
  position: absolute;
  left:30rpx;
  top:26rpx;
  color:#fff;
  z-index: 1;
  font-size: 24rpx;
  line-height: 34rpx;
  padding-left: 24rpx;
  background: url(data:image/png;base64,) no-repeat left center;
  background-size: 18rpx 22rpx;
}
.search-bar {
  width: 750rpx;
  height: 80rpx;
  position: absolute;
  left: 0;
  bottom:-40rpx;
  display: flex;
  justify-content: center;
}
.search-input {
  width:690rpx;
  height: 80rpx;
  font-size: 28rpx;
  color:#fff;
  border-radius:51rpx;
  padding: 0 94rpx;
  background: rgba(5,5,18,0.5) url(data:image/png;base64,) no-repeat 32rpx center;
  background-size:37rpx 37rpx;
}
.context-box {
  padding: 80rpx 30rpx 20rpx 30rpx;
}
.booking-frame {
  height: 330rpx;
  background-color: #111841;
}
.booking-bar-farme {
  padding: 44rpx 40rpx 36rpx 30rpx;
  background:linear-gradient(225deg,rgba(198,0,135,1) 0%,rgba(0,108,209,1) 100%);
  opacity:0.8;
  display: flex;
  justify-content: space-between;
}
.booking-text {
  display: flex;
  padding-bottom: 18rpx;
}
.booking-name {
  font-size: 40rpx;
  font-weight: 500;
  color:#fff;
}
.booking-info {
  padding-left: 18rpx;
  font-size: 24rpx;
  color:#fff;
  display: flex;
  align-items: flex-end;
  padding-bottom: 6rpx;
}
.vertical-line {
  width: 2rpx;
  height: 26rpx;
  background-color: #fff;
  margin: 0 10rpx;
}
.booking-btn {
  padding:0 12rpx 0 42rpx;
  line-height: 34rpx;
  border-radius:4rpx;
  opacity:0.5;
  width:126rpx;
  border:1rpx solid rgba(255,255,255,1);
  font-size: 24rpx;
  color:#fff;
  background: url(data:image/png;base64,) no-repeat 12rpx center;
  background-size: 24rpx 24rpx;
}
.booking-frame-r {
  width:120rpx;
  height: 120rpx;
  border-radius: 4rpx;
  border:1rpx solid rgba(255,255,255,.5);
  padding: 14rpx 0;
}
.booking-frame-r view:last-child {
  color:#fff;
  font-size: 24rpx;
  text-align: center;
}
.booking-num {
  font-size: 40rpx;
  font-weight: 500;
  color:#fff;
  text-align: center;
}
.booking-text-frame {
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 130rpx;
}
.booking-time {
  font-size: 24rpx;
  color:#fff;
  line-height: 34rpx;
}
.booking-delay {
  font-size: 24rpx;
  color:#fff;
  line-height: 34rpx;
  display: flex;
}
.booking-delay-tips {
  font-size: 20rpx;
}
/* 设置点点的层级 ,可以调整小点的位置*/
.wx-swiper-dots{position:relative;top:326rpx !important;}